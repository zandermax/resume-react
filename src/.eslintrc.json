{
  "extends": "canonical/auto",
  "rules": {
    "import/no-unassigned-import": [
			"error",
			{
				"allow": [
					"**/*.css",
					"**/*.scss",
					"@fontsource/*"
				]
			}
    ],
    "indent": [
      "error",
      "tab"
    ],
    "jsonc/indent": [
      "error",
      "tab"
    ],
    "prettier/prettier": [
			"error",
      {
        "useTabs": true
      }
    ]
  }
}